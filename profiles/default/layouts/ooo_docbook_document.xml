<?xml version="1.0"?>
<object name="ooo_docbook_document" meta_type="CPS Layout">
 <property name="layout_create_method"></property>
 <property name="layout_edit_method"></property>
 <property name="layout_view_method"></property>
 <property name="style_prefix">layout_default_</property>
 <property name="flexible_widgets"/>
 <property name="validate_values_expr"></property>
 <property name="label"></property>
 <property name="label_edit"></property>
 <property name="is_i18n">False</property>
 <widget name="Keywords" meta_type="MultiSelect Widget">
  <property name="fields">
   <element value="Keywords"/>
  </property>
  <property name="label">label_keywords</property>
  <property name="label_edit">label_keywords</property>
  <property
     name="description">This widget shows the keywords of the OOo document attached.</property>
  <property name="is_i18n">True</property>
  <property name="readonly_layout_modes"/>
  <property name="hidden_layout_modes"/>
  <property name="hidden_readonly_layout_modes"/>
  <property name="hidden_empty">True</property>
  <property name="widget_mode_expr">string:view</property>
  <property name="vocabulary">subject_voc</property>
  <property name="translated">True</property>
  <property name="sorted">True</property>
  <property name="size">4</property>
 </widget>
 <widget name="Rights" meta_type="String Widget">
  <property name="fields">
   <element value="Rights"/>
  </property>
  <property name="label_edit">label_rights</property>
  <property name="is_i18n">True</property>
  <property name="hidden_layout_modes">
   <element value="view"/>
  </property>
  <property name="display_width">30</property>
  <property name="size_max">80</property>
 </widget>
 <widget name="Source" meta_type="String Widget">
  <property name="fields">
   <element value="Source"/>
  </property>
  <property name="label_edit">label_source</property>
  <property name="is_i18n">True</property>
  <property name="hidden_layout_modes">
   <element value="view"/>
  </property>
  <property name="display_width">30</property>
  <property name="size_max">80</property>
 </widget>
 <widget name="file" meta_type="AttachedFile Widget">
  <property name="fields">
   <element value="file"/>
   <element value="file_text"/>
   <element value="file_html"/>
  </property>
  <property name="label">cpsdoc_attachedFile_label</property>
  <property name="label_edit">cpsdoc_attachedFile_label</property>
  <property name="description">cpsdoc_attachedFile_description</property>
  <property name="is_i18n">True</property>
  <property name="hidden_empty">True</property>
  <property name="css_class">ddefault</property>
  <property name="size_max">5242880</property>
  <property name="allowed_suffixes">
   <element value=".sxw"/>
   <element value=".odt"/>
  </property>
 </widget>
 <table>
  <row>
   <cell name="Source"/>
   <cell name="Rights"/>
  </row>
  <row>
   <cell name="Keywords" ncols="2"/>
  </row>
  <row>
   <cell name="file" ncols="2"/>
  </row>
 </table>
</object>
